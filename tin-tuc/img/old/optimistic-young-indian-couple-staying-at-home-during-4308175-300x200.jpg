			<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script>
function formValidate(e) {
    console.log(e.target)
  const formFields = document.querySelectorAll(
    `#${e.target.id} :is(input, textarea)`,
  )
  const formValue = {}
  formFields.forEach((input) => {
    input.onclick = () => {
      input.parentNode.setAttribute('class', '')
    }
    if (input.value == '' && input.hasAttribute('data-required'))
      input.parentNode.setAttribute('class', 'error required')
    else formValue[input.name] = input.value
  })
  if (Object.keys(formValue).length == formFields.length) return true
}

const submitHandler = async(e) => {
  e.preventDefault()
  if (formValidate(e)) {
    const formElement = e.target
    const { action, method } = formElement
    const body = new FormData(formElement)
    document.querySelector('button[type="submit"] .text').innerText = 'Đang gửi'

    axios
      .post(action, body)
      .then((response) => {
        document.querySelector('.light.center').classList.remove('item-inactive')
        document.querySelector('button[type="submit"] .text').innerText
          = 'Cùng đồng hành'
      })
      .catch((error) => {
        console.log(error)
      })
  }
}
document.querySelector('#contact-form').onsubmit = (e)=>{
    e.preventDefault()
    submitHandler(e)
}

</script>		</div>
